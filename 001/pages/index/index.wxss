/**index.wxss**/

swiper{
  height: 400rpx;
  width: 100%;
  
}
swiper image{
  width: 100%;
  height: 400rpx;
}
.nav-item{
  margin: 0px 45rpx;
   text-align: center;
}
.nav-item .img-bg{
     width: 100rpx;
    height: 100rpx;
    line-height: 100rpx;
    color: #FFFFFF;
    margin: 0px auto;
    border-radius: 50%;
    background-color: red;
}
.nav-item .title{
    font-size: 14px;
    color: #404040;
    height: 32px;
    line-height: 32px;
}

.quick-nav{
      margin: 10px 0px;
    padding: 10px 0px 2px;
    background-color: #FFFFFF;
}
/*  */
.clear:after {
	content: '';
	display: block;
	clear: both;
}

.left {
	float: left;
}

.right {
	float: right;
}

.title-font {
	font-size: 14px;
	color: #333333;
	line-height: 40px;
	height: 40px;
	padding-left: 20px;
}

.list {
	padding-left: 10px;
	padding-right: 10px;
	
}



.list .li {
	list-style: none;
	width: 50%;
	box-sizing: border-box;
	margin-bottom: 15px;
}
.list .li:nth-child(2n){
	padding-left: 4px;
} 
.list .li:nth-child(2n+1){
	padding-right: 4px;
}

.list .li image {
	width: 100%;
}

class1-title {
	height: 30px;
	line-height: 30px;
	font-size: 14px;
	color: #333;
}

class1-info {
	color: #999;
	font-size: 12px;
}

class1-info .right {
	color: #ff3333;
	font-size: 12px;
	font-weight: bold;
	line-height: 16px;
}

